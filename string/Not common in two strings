#include<stdio.h>
#include<string.h>

int i,j,k,s,f,x,count=0;
char a[20],b[20],c[20];
void strin(char a[20],char b[20],int f,int x)
{
    for(i=0;i<f;i++)
    {
        for(j=0;j<x;j++)
        {
            if(a[i]==b[j])
            {
                c[count]=b[j];
                count++;
            }
        }
    }

}
void find (char c[20],char a[20],int f,int count)
{
    for(i=0;i<count;i++)
    {
        for(j=0;j<f;j++)
        {
            if(c[i]==a[j])
            {
                a[j]=' ';
            }
        }
    }
    
    for(i=0;i<f;i++)
    {
        if(a[i]!=' ')
        {
            printf(" %c",a[i]);
        }
    }


}
void main()
{
    printf("Enter the 1 st string:");
    scanf("%s",a);
    printf("Enter the 2 nd string:");
    scanf("%s",b);
    k=strlen(a);
    s=strlen(b);
    if(k>s)
    {
         f=k;
         x=s;
         strin(a,b,f,x);
         find(c,a,f,count);
         find(c,b,x,count);
         
    }
     else
    {
         f=s;
         x=k;
         strin(b,a,f,x);
         find(c,b,f,count);
         find(c,a,x,count);
    }
    
  
}
